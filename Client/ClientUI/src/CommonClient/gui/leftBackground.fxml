<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CommonClient.controllers.leftBackgroundController">
    <children>
        <ImageView fitHeight="800.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="/assets/left300.jpg" />
            </image>
        </ImageView>
      <ImageView fx:id="btnBackNoMenu" fitHeight="60.0" fitWidth="60.0" layoutX="10.0" layoutY="10.0" onDragDetected="#OnClickBackNoMenuButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/assets/backButton.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
